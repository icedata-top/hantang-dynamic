    "@typescript-eslint/parser": "^8.23.0",
    "@vercel/ncc": "^0.38.3",
    "@yao-pkg/pkg": "^6.3.0",
    "eslint": "^9.19.0",
    "eslint-config-prettier": "^10.0.1",
    "prettier": "^3.4.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3"
